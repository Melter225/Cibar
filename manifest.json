{
  "manifest_version": 3,
  "name": "Cibar",
  "version": "1.0",
  "description": "A Chrome extension built to combat cyberbullying",
  "action": {
    "default_popup": "src/popup.html"
  },
  "permissions": ["activeTab", "scripting", "storage", "tabs", "windows"],
  "background": {
    "service_worker": "dist/background.js"
  }
}
